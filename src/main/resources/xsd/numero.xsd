<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:tns="http://www.alejandro-luyo.com/ws/objects"
           targetNamespace="http://www.alejandro-luyo.com/ws/objects"
           elementFormDefault="qualified">
    <xs:complexType name="Numero">
        <xs:sequence>
            <xs:element name="vocal" type="xs:string"/>
            <xs:element name="numero" type="xs:int"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="NumeroRequest">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="numero" type="xs:int"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="NumeroResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="vocal" type="xs:string"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>